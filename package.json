{
  "name": "chaterm",
  "version": "0.8.0",
  "description": "AI Terminal with Claude",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "cross-env NODE_ENV=production electron-vite build",
    "build:cn": "cross-env NODE_ENV=production electron-vite build",
    "build:win": "npm run build && electron-builder --win",
    "build:win:cn": "npm run build:cn && electron-builder --win --config electron-builder.cn.yml",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "build": {
    "appId": "com.chaterm.app",
    "productName": "ChatTerm-CN",
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tanlongtruong7-star/AI-Terminal-with-Claude.git"
  },
  "dependencies": {
    "ant-design-vue": "^4.2.1",
    "@ant-design/icons-vue": "^7.0.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-i18n": "^9.10.2",
    "pinia": "^2.1.7",
    "ssh2": "^1.15.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-webgl": "^0.16.0",
    "xterm-addon-web-links": "^0.9.0",
    "better-sqlite3": "^9.4.3",
    "axios": "^1.6.7",
    "@electron-toolkit/utils": "^3.0.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "uuid": "^9.0.1",
    "iconv-lite": "^0.6.3",
    "adm-zip": "^0.5.10",
    "@anthropic-ai/sdk": "^0.20.1",
    "@anthropic-ai/bedrock-sdk": "^0.10.0",
    "@aws-sdk/client-bedrock-runtime": "^3.556.0",
    "@aws-sdk/credential-providers": "^3.556.0",
    "@aws-sdk/node-http-handler": "^3.370.0",
    "@aws-crypto/client-node": "^4.0.0",
    "@bufbuild/protobuf": "^1.8.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "openai": "^4.29.1",
    "ollama": "^0.5.0",
    "node-pty": "^1.0.0",
    "chokidar": "^3.6.0",
    "fast-deep-equal": "^3.1.3",
    "clone-deep": "^4.0.1",
