    "p-wait-for": "^5.0.2",
    "serialize-error": "^11.0.3",
    "dotenv": "^16.4.5",
    "posthog-node": "^4.0.1",
    "p-timeout": "^6.1.2",
    "get-folder-size": "^4.0.0",
